{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false,
      "private": true
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false,
      "private": true
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false,
      "private": true
    },
    "classes": {
      "collection": "class",
      "via": "trainers"
    },
    "posts": {
      "collection": "post",
      "via": "authors"
    },
    "stories": {
      "collection": "story",
      "via": "author"
    },
    "shortInfo": {
      "type": "dynamiczone",
      "components": [
        "info.trainer-info"
      ]
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "socialMedia": {
      "type": "dynamiczone",
      "components": [
        "socials.facebook",
        "socials.instagram",
        "socials.twitter"
      ]
    },
    "schedule": {
      "type": "dynamiczone",
      "components": [
        "trainer-schedule.tuesday",
        "trainer-schedule.monday",
        "trainer-schedule.wednesday",
        "trainer-schedule.thursday",
        "trainer-schedule.friday"
      ]
    }
  }
}
